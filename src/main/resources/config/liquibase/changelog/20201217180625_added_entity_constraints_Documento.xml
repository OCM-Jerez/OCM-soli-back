<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Documento.
    -->
    <changeSet id="20201217180625-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="solicitud_id"
                                 baseTableName="documento"
                                 constraintName="fk_documento_solicitud_id"
