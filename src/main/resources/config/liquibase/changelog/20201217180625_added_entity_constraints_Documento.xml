        <addForeignKeyConstraint baseColumnNames="gestion_id"
                                 baseTableName="documento"
                                 constraintName="fk_documento_gestion_id"
                                 referencedColumnNames="id"
                                 referencedTableName="gestion"/>

    </changeSet>
</databaseChangeLog>
